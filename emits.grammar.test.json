{
    "name": "test grammar",
    "description": "this is a test",
    "extension": [".scss"],
    "grammar": [
        {
            "pattern": "@param (?P<pname>.+?) (?P<pvalue>.+)",
            "match": {
                "pname": {
                    "grammar": [
                        {
                            "pattern": "(?P<any>(.+)?)",
                            "match": {
                                "any": {
                                    "set": {
                                        "keyword": "{{any}}"
                                    }
                                }
                            }
                        },
                        {
                            "pattern": "^(?P<flag>[d|v]oo)(?P<foo>(.+)?)",
                            "match": {
                                "foo": {
                                    "set": {
                                        "keyword": "prefix{{foo}}",
                                        "flags": "test,was{{flag}}"
                                    }
                                }
                            }
                        }
                    ]
                },
                "pvalue": {
                    "set": {
                        "value": "{{pvalue}}"
                    }
                }
            }
        }
    ]
}
